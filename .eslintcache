/volumes?q=${query}&maxReuslts=${maxResults}startIndex=${startIndex}`\n        )\n        .then((res) => {\n          if (startIndex > res.data.totalItems || startIndex < 1) {\n            toast.error(\n              `Max results must be between 1 and ${res.data.totalItems}`\n            );\n          } else {\n            if (res.data.items.length > 0) {\n              setCards(res.data.items);\n              setLoading(false);\n            }\n          }\n        })\n        .catch((err) => {\n          setLoading(true);\n          toast.error(`${err.response.data.error.message}`);\n        });\n    }\n  };\n\n  const handleCards = () => {\n    console.log(cards);\n    const items = cards.map((item, io) => {\n      let thumbnail = \"\";\n      if (item.volumeInfo.imageLinks.thumbnail) {\n        thumbnail = item.volumeInfo.imageLinks.thumbnail;\n      }\n\n      return (\n        <div className=\"col-lg-4\" key={item.id}>\n          <BookCards\n            thumbnail={thumbnail}\n            title={item.volumeInfo.title}\n            pageCount={item.volumeInfo.pageCount}\n            language={item.volumeInfo.language}\n            authors={item.volumeInfo.authors}\n            publisher={item.volumeInfo.publisher}\n            description={item.volumeInfo.description}\n            previewLink={item.volumeInfo.previewLink}\n            infoLink={item.volumeInfo.infoLink}\n          />\n        </div>\n      );\n    });\n    if (loading) {\n      return (\n        <div className=\"d-flex justify-content -center mt-3\">\n          <Spinner style={{ width: \"3rem\", height: \"3rem\" }} />\n        </div>\n      );\n    } else {\n      return (\n        <div className=\"container ,my-5\">\n          <div className=\"row\">{items}</div>\n        </div>\n      );\n    }\n  };\n\n  return (\n    <div className=\"search\">\n      <div className=\"journal__header\">\n        <h3>Resources</h3>\n      </div>\n      <div className=\"search_area\">\n        <InputGroup size=\"lg\" className=\"mb-3\">\n          <Input\n            placeholder=\"Search\"\n            value={query}\n            onChange={(e) => setQuery(e.target.value)}\n          />\n          <InputGroupAddon addonType=\"append\" onClick={handleSubmit}>\n            <Button variant=\"contained\" color=\"secondary\">\n              Search\n            </Button>\n          </InputGroupAddon>\n        </InputGroup>\n      </div>\n      <div className=\"d-flex\">\n        <FormGroup className=\"ml-5\">\n          <Input\n            type=\"number\"\n            id=\"maxResults\"\n            placeholder=\"Max Results\"\n            value={maxResults}\n            onChange={(e) => setMaxResults(e.target.value)}\n          />\n        </FormGroup>\n        <FormGroup className=\"ml-5\">\n          <Input\n            type=\"number\"\n            id=\"startIndex\"\n            placeholder=\"Start Index\"\n            value={startIndex}\n            onChange={(e) => setStartIndex(e.target.value)}\n          />\n        </FormGroup>\n        <ToastContainer />\n      </div>\n      {handleCards()}\n    </div>\n  );\n}\n\nexport default Resources;\n","/mnt/c/Users/natha/github/softwareproject-final-year/src/Components/Message.js",[],"/mnt/c/Users/natha/github/softwareproject-final-year/src/Components/SidebarResource.js",[],"/mnt/c/Users/natha/github/softwareproject-final-year/src/Components/BookCards.js",[],"/mnt/c/Users/natha/github/softwareproject-final-year/src/Components/Journal-Import.js",[],{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","severity":1,"message":"69","line":7,"column":8,"nodeType":"70","messageId":"71","endLine":7,"endColumn":23},{"ruleId":"68","severity":1,"message":"72","line":5,"column":20,"nodeType":"70","messageId":"71","endLine":5,"endColumn":29},"no-native-reassign",["73"],"no-negated-in-lhs",["74"],"no-unused-vars","'SidebarResource' is defined but never used.","Identifier","unusedVar","'useEffect' is defined but never